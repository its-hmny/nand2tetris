/**
 * Or gate:
 * out = (((a == 1) || (b == 1))), 1, 0) 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS: // Available gate(s): Nand, Not, And
    Not(in=a, out=NotA);
    Not(in=b, out=NotB);
    Nand(a=NotA, b=NotB, out=out);
}
