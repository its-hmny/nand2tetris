// File name: projects/01/Mux.hdl
/** 
 * Multiplexor:
 * out = ((sel == 0), a, b)
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS: // Available gate(s): Nand, Not, And, Or, Xor
    Not(in=sel, out=NotSel);
    And(a=a, b=NotSel, out=AAndNotSel);
    And(a=b, b=sel, out=BAndSel);
    Or(a=AAndNotSel, b=BAndSel, out=out);
}