/**
 * Demultiplexor:
 * [a, b] = ((sel == 0), [in, 0], [0, in])
 */
CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS: // Available gate(s): Nand, Not, And, Or, Xor, Mux
    Not(in=sel, out=NotSel);
    And(a=in, b=NotSel, out=a);

    And(a=in, b=sel, out=b);
}
